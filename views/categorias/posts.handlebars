<h1>{{categoria.nome}}</h1>
{{#each posts}}
<div class="card mt-3">
  <div class="card-body">
    <h3>{{titulo}}</h3>
    <p>{{descrição}}</p>
    <a href="/post/{{slug}}"><button class="btn btn-success">Ler mais</button></a>
    <hr>
    <small>Data de publicação: {{data}}</small>
  </div>
</div>
{{else}}
  <h5>Nenhuma postagem</h5>
{{/each}}